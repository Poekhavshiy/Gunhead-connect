#include <winresrc.h>
IDI_ICON1 ICON "@ICON_ABSOLUTE_PATH@"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
 PRODUCTVERSION @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
 FILEFLAGSMASK 0x3fL
 FILEFLAGS 0x0L
 FILEOS 0x40004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "@PROJECT_AUTHOR@"
            VALUE "FileDescription", "Gunhead Connect"
            VALUE "FileVersion", "@PROJECT_VERSION@"
            VALUE "LegalCopyright", "Copyright (C) 2025 @PROJECT_AUTHOR@"
            VALUE "ProductName", "Gunhead Connect"
            VALUE "ProductVersion", "@PROJECT_VERSION@"
            VALUE "OriginalFilename", "Gunhead Connect.exe"
            VALUE "InternalName", "Gunhead Connect"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END